{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "rootDir": "./src",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
